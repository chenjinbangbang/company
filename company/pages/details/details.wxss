/* pages/details/details.wxss */
@import "/wxParse/wxParse.wxss"; 

swiper{ height:200px;}
.slide-image {
  width:100%;
  /* height:500rpx; */
  /* border:1px solid #f00; */
}

.details {
  padding-bottom:100rpx;
}

.details .price {
  height: 100rpx;
  display:flex;
  align-items:center;
}

.details .price .price1 {
  font-size: 46rpx;
  color: #f00;
  font-weight: bold;
  margin: 0 20rpx;
}

.details .price .price2 {
  font-size: 32rpx;
  text-decoration: line-through;
  margin-top:10rpx;
}

.details .title {
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  font-weight:bold;
  padding:10rpx 20rpx;
  font-size:44rpx;
}

.details .content {
  padding:20rpx;
}

.contact{
  position:fixed;
  width:100%;
  bottom:0;
  background-color:#2CA244;
  border-radius:0;
}
.contact>view{ color:#fff; display:inline; position:relative;}
.contact>view .phone{ 
  width:55rpx; height:55rpx; position:absolute; left:-100rpx; top:0; 
background-image: url("data:image/svg+xml;base64,");
   background-size: cover;
   }


