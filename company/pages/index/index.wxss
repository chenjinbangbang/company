.home{
  width:90%;
  margin:100rpx auto 0 auto;
  display:flex;
  justify-content:space-between;
  flex-wrap: wrap;
}
.home .navigator-hover{
  background-color:#fff;
}
.home navigator{
  width:300rpx;
  height:300rpx;
  border:1rpx solid #000;
  border-radius:10px;
  text-align:center;
  font-size:30rpx;
  margin-bottom:60rpx;
}
.home navigator image{
  width:180rpx;
  height:180rpx;
